@model ViewModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    @foreach (var item in @Model.Prices)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Coderab)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cost)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |

                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
    }

</table>

<p>
    <a asp-action="Create1">Create New</a>
</p>

<table class="table">


    @foreach (var item in @Model.MoveDBs)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Coderem)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Coderab)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                <a asp-action="Edit1" asp-route-id="@item.Id">Edit</a>
                |
                <a asp-action="Delete1" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
    }

</table>